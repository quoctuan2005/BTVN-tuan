#include <iostream>
#include <algorithm>
using namespace std;
    struct student{
        int age;
        string first_name;
        string last_name;
        int standard;
   };
   int main() {
    student a;
    cin >> a.age >> a.first_name >> a.last_name >> a.standard;
    cout << a.age <<" " << a.first_name << " " << a.last_name << " " << a.standard;
    
    return 0;
}
