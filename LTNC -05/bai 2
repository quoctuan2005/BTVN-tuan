#include <bits/stdc++.h>
using namespace std;
int minDeleteToMakeEqual(vector<int> &arr)
{
    unordered_map<int, int> freq;
    for (int num : arr)
    {
        freq[num]++;
    }
    int maxFreq = 0;
    for (const auto &pair : freq)
    {
        maxFreq = max(maxFreq, pair.second);
    }
    return arr.size() - maxFreq;
}
int main()
{
    int n;
    cin >> n;
    vector<int> arr(n);
    for (int i = 0; i < n; i++)
    {
        cin >> arr[i];
    }
    int result = minDeleteToMakeEqual(arr);
    cout << result << endl;
    return 0;
}
