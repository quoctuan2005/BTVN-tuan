#include <bits/stdc++.h>
using namespace std;
string kangaroo(int x1, int v1, int x2, int v2) {
    if (v2 >= v1)
    {
        return "NO";
    } else if((x1-x2)%(v2-v1) ==0 && (x1-x2)/(v2-v1)>0){
        return "YES";
     } else return "NO";
return "YES";
}

int main()
{
    int x1, v1, x2, v2;
    cin >>x1 >> v1 >> x2 >> v2;
    cout << kangaroo(x1,v1,x2,v2);
}
